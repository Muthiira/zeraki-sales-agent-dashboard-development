{
	"version": 2,
	"builds": [
	  {
		"src": "api/backend/json-server.js",
		"use": "@vercel/node"
	  },
	  {
		"src": "frontend/package.json",
		"use": "@vercel/static-build",
		"config": { "distDir": "build" }
	  }
	],
	"routes": [
	  { "src": "/api/(.*)", "dest": "api/backend/json-server.js" },
	  { "src": "/(.*)", "dest": "build/$1" }
	]
  }
  